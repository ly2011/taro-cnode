{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "services/*": [
        "./src/services/*"
      ],
      "models/*": [
        "./src/models/*"
      ],
      "utils/*": [
        "./src/utils/*"
      ],
      "config/*": [
        "./src/config/*"
      ],
      "pages/*": [
        "./src/pages/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "assets/*": [
        "./src/assets/*"
      ]
    },
    "lib": ["esnext", "dom"]
  }
}
